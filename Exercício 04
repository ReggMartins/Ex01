import java.util.Scanner;


public class Main {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		int num;
		
		System.out.println("Informe o n√∫mero! ");
		num = sc.nextInt();
		
		if (num < 0) {
			System.out.println("Negativo");
		}
		else {
			System.out.println("Positivo");
		}
		
	
		sc.close();
	
	}

}
